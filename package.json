{
  "name": "cookie_or_death",
  "version": "1.0.0",
  "description": "Alien Translater",
  "homepage": "https://nwewave32.github.io/cookie_or_death/",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist .parcel-cache && GENERATE_SOURCEMAP=false npx parcel build src/*.html --public-url /cookie_or_death/ --no-minify src/IMG/metro/*.png --no-content-hash",
    "predeploy": "npm run build",
    "copy-assets": "cp -r src/IMG/metro",
    "deploy": "npm run build && npm run copy-assets && npx gh-pages -d dist"
  },
  "targets": {
    "default": {
      "distDir": "./dist",
      "publicUrl": "./",
      "context": "browser",
      "engines": {
        "browsers": "> 0.25%, not dead"
      },
      "includeNodeModules": true,
      "outputFormat": "esmodule",
      "sourceMaps": false,
      "minify": true
    },
    "images": {
      "distDir": "./dist/images",
      "publicUrl": "./images",
      "context": "browser",
      "engines": {
        "browsers": "> 0.25%, not dead"
      },
      "includeNodeModules": true,
      "outputFormat": "esmodule",
      "sourceMaps": false,
      "minify": true,
      "contentHash": false
    }
  },
  "customDistStructure": {
    "config": {
      ".js": "js",
      "images": [
        ".jpg",
        ".png"
      ],
      ".fileExtension": "folder/in/dist",
      "folder/in/dist": [
        ".file",
        ".extensions"
      ]
    },
    "options": {
      "development": true
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "parcel": "^2.12.0",
    "parcel-bundler": "^1.12.5",
    "parcel-plugin-custom-dist-structure": "^1.1.20",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10"
  }
}
